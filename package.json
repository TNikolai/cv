{
  "name": "cv",
  "version": "1.0.0",
  "description": "CV",
  "main": "./server/index.js",
  "scripts": {
    "start": "webpack-dev-server --config ./webpack/dev.config.js --env development",
    "start-server": "nodemon --experimental-modules --es-module-specifier-resolution=node ./server/index",
    "webpack-server": "webpack --config ./webpack/server.config.js",
    "build": "npm run build-static-html & npm run build-client",
    "build-static-html": "webpack --config ./webpack/prod.static.render.js && rm build/ignore.js",
    "build-client": "webpack --config ./webpack/prod.client.config.js",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "keywords": [],
  "author": "Nicolai Trandafil",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.4",
    "css-loader": "^1.0.1",
    "gh-pages": "^3.2.3",
    "html-webpack-plugin": "^5.3.2",
    "postcss-loader": "^3.0.0",
    "react-hot-loader": "^4.13.0",
    "static-site-generator-webpack-plugin": "^3.4.2",
    "style-loader": "^0.23.1",
    "webpack": "^5.64.3",
    "webpack-cli": "^4.9.1",
    "copy-webpack-plugin": "^6.4.1",
    "webpack-dev-server": "^4.5.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-emotion": "^11.0.0"
  },
  "dependencies": {
    "express": "^4.17.1",
    "nodemon": "^2.0.3",
    "react": "next",
    "react-dom": "next",
    "react-dom-server": "0.0.5",
    "react-router-dom": "^4.3.1",
    "@emotion/cache": "^11.4.0",
    "@emotion/react": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "emotion-theming": "^11.0.0",
    "style-loader": "^2.0.0"
  }
}
